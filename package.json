{
  "name": "aican-web-development-agent",
  "version": "1.0.0",
  "description": "Web Development Agent for managing site, web app, and UI development with CI/CD integration",
  "main": "index.js",
  "scripts": {
    "start": "node server.js",
    "dev": "node server.js",
    "build": "npm run test && npm run lint",
    "test": "jest --coverage --passWithNoTests",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.js",
    "lint:fix": "eslint src/**/*.js --fix",
    "deploy": "node scripts/deploy.js",
    "report-status": "node scripts/report-status.js"
  },
  "dependencies": {
    "express": "^4.18.2",
    "axios": "^1.4.0",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "jest": "^29.6.1",
    "eslint": "^8.44.0",
    "supertest": "^6.3.3"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/**/*.test.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": 90
      }
    }
  },
  "keywords": [
    "web-development",
    "agent",
    "ci-cd",
    "vercel",
    "github",
    "vscode"
  ],
  "author": "AiCan Web Development Agent",
  "license": "MIT"
}